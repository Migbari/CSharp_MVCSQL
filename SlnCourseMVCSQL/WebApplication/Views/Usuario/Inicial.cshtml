@model IEnumerable<BDProjetoDominio.Usuario>

@{
    ViewBag.Title = "Inicial";
}
<h2>Página do Usuário</h2><br>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cargo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cargo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data)
            </td>
            <td>
                @Html.ActionLink("Alterar", "Editar", new { id = item.Id }) |
                @Html.ActionLink("Detalhar", "Detalhes", new { id = item.Id }) |
                @Html.ActionLink("Deletar", "Excluir", new { id = item.Id })
            </td>
        </tr>
    }

</table>
